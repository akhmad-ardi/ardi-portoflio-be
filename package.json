{
  "name": "ardi-portofolio-be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "DEBUG=ardi-portofolio-be:* npm start",
    "sequelize": "npx sequelize-cli",
    "sequelize:migrate": "npx sequelize-cli db:migrate",
    "sequelize:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "sequelize:rollback": "npx sequelize-cli db:migrate:undo",
    "sequelize:seed": "npx sequelize-cli seed:generate --name",
    "sequelize:seed:all": "npx sequelize-cli db:seed:all",
    "sequelize:model": "npx sequelize-cli model:generate --name",
    "sequelize:migration": "npx sequelize-cli migration:generate --name"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.21.2",
    "express-handlebars": "^8.0.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.12.0",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  }
}
